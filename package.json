{
  "name": "bluepeak",
  "version": "1.0.0",
  "description": "bluepeak",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:smoke": "playwright test --grep @smoke",
    "test:headed": "playwright test --headed",
    "clean": "node -e \"const fs=require('fs'); const path=require('path'); ['allure-results', 'allure-report', 'test-results', 'playwright-report'].forEach(dir => {try {fs.rmSync(path.join(__dirname, dir), {recursive: true, force: true});} catch (e) {console.error(e)}});\"",
    "allure:generate": "allure generate allure-results --clean",
    "allure:open": "allure open allure-report",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@playwright/test": "^1.40.0",
    "dotenv": "^17.2.3",
    "playwright": "^1.40.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^22.19.3",
    "allure-commandline": "^2.37.0",
    "allure-playwright": "^3.5.0",
    "typescript": "^5.0.0"
  }
}